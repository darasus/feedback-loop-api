datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id Int @default(autoincrement()) @id
    email String @unique
    name String?
}

model Organisation {
    id Int @default(autoincrement()) @id
}

model Feedback {
    id Int @default(autoincrement()) @id
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    message String
}